<style>
#galleryCarousel {
  height: 520px;
  background: #f5f5f5; /* light neutral background */
}

#galleryCarousel .carousel-inner,
#galleryCarousel .carousel-item {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.gallery-box {
  margin-bottom: 15px;
}

.gallery-box-inner {
  height: 200px;
  background: #f4f6f9;
  display: flex;
  align-items: center;
  justify-content: center;
}

.gallery-box-inner img {
  max-width: 100%;
  max-height: 100%;
  object-fit: contain;
}
/* Exact 5-column layout */
.leader-row {
  display: flex;
  justify-content: space-between;
  text-align: center;
}

.leader-col {
  width: 20%; /* 100 / 5 = exact */
  padding: 0 8px;
}

/* Image box */
.leader-img {
  height: 160px;
  background: #f4f6f9;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 4px;
}

.leader-img img {
  max-width: 100%;
  max-height: 100%;
  object-fit: contain;
}

.leader-text {
  margin-top: 8px;
  font-size: 14px;
  line-height: 1.4;
}

/* Responsive fallback */
@media (max-width: 992px) {
  .leader-row {
    flex-wrap: wrap;
  }
  .leader-col {
    width: 33.33%;
    margin-bottom: 20px;
  }
}

@media (max-width: 576px) {
  .leader-col {
    width: 50%;
  }
}

