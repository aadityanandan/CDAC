<script>
function Gallery(config) {
  this.path = config.path;
  this.total = config.total;
  this.perPage = 9;
  this.page = 1;

  this.container = document.getElementById(config.container);
  this.pageInfo = document.getElementById(config.pageInfo);
  this.prevBtn = document.getElementById(config.prev);
  this.nextBtn = document.getElementById(config.next);

  this.render = () => {
    this.container.innerHTML = '';

    const start = (this.page - 1) * this.perPage + 1;
    const end = Math.min(start + this.perPage - 1, this.total);

    for (let i = start; i <= end; i++) {
      const col = document.createElement('div');
      col.className = 'col-md-4 gallery-box';

      const box = document.createElement('div');
      box.className = 'gallery-box-inner';

      const img = document.createElement('img');
      img.src = `${this.path}/Picture${i}.jpg`;
      img.loading = 'lazy';

      box.appendChild(img);
      col.appendChild(box);
      this.container.appendChild(col);
    }

    this.pageInfo.textContent =
      `Page ${this.page} of ${Math.ceil(this.total / this.perPage)}`;

    this.prevBtn.disabled = this.page === 1;
    this.nextBtn.disabled = end >= this.total;
  };

  this.prevBtn.onclick = () => {
    if (this.page > 1) {
      this.page--;
      this.render();
    }
  };

  this.nextBtn.onclick = () => {
    if (this.page * this.perPage < this.total) {
      this.page++;
      this.render();
    }
  };

  this.render();
}

// INIT GALLERIES
new Gallery({
  path: '/gallery/SDCold',
  total: 7,
  container: 'oldGallery',
  pageInfo: 'oldPageInfo',
  prev: 'oldPrev',
  next: 'oldNext'
});

new Gallery({
  path: '/gallery/SDCnew',
  total: 12,
  container: 'newGallery',
  pageInfo: 'newPageInfo',
  prev: 'newPrev',
  next: 'newNext'
});
</script>


